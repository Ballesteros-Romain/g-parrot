{% extends 'base.html.twig' %}

{% block title %}Garage V.parrot - Contact{% endblock %}

{% block body %}
    <h2>contact</h2>
    <div class="form-groupe">
        {{form_start(contactForm)}}
        {{form_row(contactForm.email)}}
        {{form_row(contactForm.name)}}
        {{form_row(contactForm.surname)}}
        {{form_row(contactForm.tel)}}
        {{form_row(contactForm.message)}}
        <button type="submit" class="btn btn2 mt-3 mb-3">Envoyer</button>
        {{form_end(contactForm)}}
    </div>
{% endblock %}
